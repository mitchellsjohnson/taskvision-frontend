{
  "name": "taskvision-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dev.taskvision.app",
  "dependencies": {
    "@auth0/auth0-react": "^1.10.2",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "cross-env PORT=4040 dotenv -e .env.local react-scripts start",
    "start:dev": "cross-env PORT=4040 dotenv -e .env.dev react-scripts start",
    "start:prod": "cross-env PORT=4040 dotenv -e .env.prod react-scripts start",
    "build": "cross-env dotenv -e .env.local react-scripts build",
    "build:dev": "cross-env dotenv -e .env.dev react-scripts build",
    "build:prod": "cross-env dotenv -e .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.2.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "json-server": "^0.17.0",
    "prettier": "^2.7.1",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
